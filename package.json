{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "repository": "https://github.com/MaxKim-J/react-boilerplate.git",
  "author": "Max Jonghyuk Kim",
  "license": "MIT",
  "scripts": {
    "start:dev": "webpack-dev-server --env development --config ./config/webpack/webpack.config.dev.js --open",
    "start:prod": "webpack-dev-server --env production --config ./config/webpack/webpack.config.dev.js --open",
    "build:dev": "webpack --config --env development ./config/webpack/webpack.config.js",
    "build:prod": "webpack --config --env production ./config/webpack/webpack.config.js",
    "type-check": "tsc",
    "bundle-analyze": "webpack-bundle-analyzer ./analysis/bundle-stats.json"
  },
  "dependencies": {
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "typecript": "^0.0.1-security",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^5.2.7",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-plugin": "^4.4.0",
    "style-loader": "^3.1.0",
    "webpack": "^4.37.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.11.2",
    "webpack-dotenv-plugin": "^2.1.0"
  }
}
